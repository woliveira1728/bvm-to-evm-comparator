
contract MessageStorageSCrypt {
  @state bytes message;
  @state PubKey owner;
  public function updateMessage(bytes newMessage, Sig sig, SigHashPreimage __scrypt_ts_txPreimage) {
    require(Tx.checkPreimageSigHashType(__scrypt_ts_txPreimage, SigHashType(b'41')));
    this.message = newMessage;
    require(true);
  }
  function buildStateOutput(int amount) : bytes {  return Utils.buildOutput(this.getStateScript(), amount);}
}